{
    "name": "nikcani/smart-inventory-snipe-it-plugin",
    "description": "",
    "keywords": [
        "nikcani",
        "smart-inventory-snipe-it-plugin"
    ],
    "homepage": "https://github.com/nikcani/smart-inventory-snipe-it-plugin",
    "license": "GPL-2.0-or-later",
    "type": "library",
    "authors": [
        {
            "name": "Niklas Canisius",
            "email": "niklascanisius@me.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Nikcani\\SmartInventorySnipeItPlugin\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nikcani\\SmartInventorySnipeItPlugin\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nikcani\\SmartInventorySnipeItPlugin\\SmartInventorySnipeItPluginServiceProvider"
            ],
            "aliases": {
                "SmartInventorySnipeItPlugin": "Nikcani\\SmartInventorySnipeItPlugin\\SmartInventorySnipeItPluginFacade"
            }
        }
    }
}
